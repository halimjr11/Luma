<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LumaInputField">
        <attr name="inputTypeMode" format="enum">
            <enum name="username" value="0" />
            <enum name="email" value="1" />
            <enum name="password" value="2" />
        </attr>

        <attr name="hintText" format="string" />

        <attr name="enableAutoValidate" format="boolean" />
    </declare-styleable>
</resources>